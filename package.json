{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "./build/server",
  "scripts": {
    "test": "mocha -r ts-node/register ./src/tests/*$NAME*.test.ts -t 10000 --exit",
    "test:coverage": "nyc npm run test",
    "test:coverage:json": "nyc --reporter=json-summary npm run test",
    "build": "/bin/sh tsc_eval.sh",
    "db:reset": "npx prisma generate && npx prisma migrate dev --name init && npm install @prisma/client && npx prisma db push",
    "prestart": "npm run build && npm run db:reset",
    "start": "node .",
    "predev": "npm run build && npm run db:reset",
    "dev": "ts-node ./src/server.ts"
  },
  "author": "Alessandro Fuhr de Mello",
  "license": "ISC",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@types/chai": "4.3.0",
    "@types/chai-http": "4.2.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mocha": "9.1.0",
    "@types/node": "^16.11.7",
    "@types/sinon": "10.0.11",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "chai": "4.3.6",
    "chai-http": "4.3.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "mocha": "9.2.1",
    "nodemon": "^2.0.14",
    "nyc": "15.1.0",
    "prisma": "^3.11.1",
    "sinon": "13.0.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "@types/cors": "^2.8.12",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.2.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1"
  }
}
